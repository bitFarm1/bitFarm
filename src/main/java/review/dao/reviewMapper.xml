<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="reviewSQL">

	<select id="getItemReviewList" parameterType="int" resultType="review">
		select * from review where item_id=#{item_id}
	</select>

	<select id="getItemName" parameterType="String" resultType="String">
		select item_name from item where item_id=#{item_id}
	</select>
	
</mapper>